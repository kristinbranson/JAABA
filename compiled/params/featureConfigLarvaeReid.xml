    <a_mm trans_types="relative,none" type="appearance"/>
    <area trans_types="relative,none" type="appearance"/>
    <b_mm trans_types="relative,none" type="appearance"/>
    <bearingangforcurvingrate trans_types="relative,abs,none" type="odorsource"/>
    <bearingangle trans_types="relative,abs,none" type="odorsource"/>
    <bias trans_types="relative,abs,none" type="locomotion"/>
    <bodyangle trans_types="relative,abs,none" type="position"/>
    <bodyanglespeed trans_types="relative,abs,none" type="locomotion"/>
    <cdotc trans_types="relative,abs,none" type="odorsource"/>
    <centerspeed trans_types="relative,none" type="locomotion"/>
    <concheadspeed trans_types="relative,abs,none" type="odorsource"/>
    <crab trans_types="relative,none" type="locomotion"/>
    <curve trans_types="relative,none" type="appearance"/>
    <curvingrate trans_types="relative,abs,none" type="locomotion"/>
    <cx trans_types="relative,none" type="position"/>
    <cy trans_types="relative,none" type="position"/>
    <dfromsource trans_types="relative,none" type="odor_position"/>
    <dirchange trans_types="relative,none" type="unknown"/>
    <hcdist trans_types="relative,abs,none" type="appearance"/>
    <headangle trans_types="relative,abs,none" type="appearance"/>
    <heading trans_types="relative,abs,none" type="position"/>
    <headspeed trans_types="relative,none" type="locomotion"/>
    <headvecx trans_types="relative,abs,none" type="position"/>
    <headvecy trans_types="relative,abs,none" type="position"/>
    <headx trans_types="relative,none" type="position"/>
    <heady trans_types="relative,none" type="position"/>
    <kink trans_types="relative,none" type="appearance"/>
    <midline trans_types="relative,none" type="appearance"/>
    <orient trans_types="relative,abs,none" type="position"/>
    <skelL trans_types="relative,none" type="appearance"/>
    <source trans_types="relative,none" type="odorsource"/>
    <sourceangle trans_types="relative,abs,none" type="odorsource"/>
    <speedang trans_types="relative,none" type="locomotion"/>
    <speedmms trans_types="relative,none" type="locomotion"/>
