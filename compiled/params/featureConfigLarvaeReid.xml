    <x_mm trans_types="relative,none" type="position"/>
    <y_mm trans_types="relative,none" type="position"/>
    
    <event_idxGoodCast trans_types="relative,none" type="event"/>
    <event_idxGoodNeg trans_types="relative,none" type="event"/>
