    <min/>
    <max/>
    <hist hist_edges="-1,1"/>
    <change change_window_radii="1,3"/>
    <std/>
    <harmonic num_harmonic="5"/>
    <diff_neighbor_mean/>
    <diff_neighbor_min/>
    <diff_neighbor_max/>
    <zscore_neighbors/>
  </more>  
  
